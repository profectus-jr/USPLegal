<h2>Visualização de Usuário</h2>


<table class="table">
    <tr>
        <th>ID</th>
        <td><%= @user.id%></td>
    </tr>
    <tr>
        <th>Nome</th>
        <td><%= @user.name %></td>
    </tr>
    <tr>
        <th>CPF</th>
        <td><%= @user.cpf %></td>
    </tr>
    <tr>
        <th>Número USP</th>
        <td><%= @user.nusp %></td>
    </tr>
    <tr>
        <th>Tipo</th>
        <td><%= @user.kind %></td>
    </tr>
    <tr>
        <th>E-mail</th>
        <td><%= @user.email %></td>
    </tr>
</table>
