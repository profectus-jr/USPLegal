<font size="3"><b>Nova vistoria</b></font>

<%= nested_form_for @group do |f| %>
  </br>
  <%= f.label :name, "Nome (Padrao: Orgao.Sigla/Ano.Mes)"%>
  <%= f.text_field :name %>

  <%= f.label :building_id, "Localização" %>
  <%= f.select(:building_id, options_for_select(@predios, @predio)) %>

  <%= f.label :vist_date, "Data"%>
  <%= f.date_field :vist_date %>

  <br/>
  <%= f.submit "Criar", :class => "btn default-button" %>
  
<%end%>
